{
  "name": "indiekit-preset-fff",
  "version": "0.4.0-alpha.0",
  "author": "Ëóç+85CD",
  "license": "WTFPL",
  "keywords": [
    "fff",
    "fff-flavored-frontmatter",
    "indiekit",
    "indiekit-plugin",
    "indiekit-preset",
    "indieweb",
    "typescript"
  ],
  "description": "FFF Flavored Frontmatter publication preset for Indiekit.",
  "repository": {
    "type": "git",
    "url": "https://github.com/importantimport/fff.git",
    "directory": "packages/indiekit-preset-fff"
  },
  "homepage": "https://fff.js.org",
  "bugs": "https://github.com/importantimport/fff/issues",
  "type": "module",
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "unbuild": {
    "entries": [
      "src/index"
    ],
    "clean": true,
    "declaration": true,
    "rollup": {
      "emitCJS": true
    }
  },
  "scripts": {
    "build": "unbuild"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "fff-flavored-frontmatter": "workspace:*"
  }
}