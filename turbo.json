{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".vitepress/dist/**",
        "dist/**"
      ]
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env",
    "tsconfig.json",
    "tsup.config.ts"
  ]
}